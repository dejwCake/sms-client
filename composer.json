{
    "name": "dejwcake/sms-client",
    "description": "A generic SMS client library. Supports multiple swappable drivers.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "sms"
    ],
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com",
            "role": "Original Developer"
        },
        {
            "name": "David BÄ›hal",
            "email": "behal.david@gmail.com",
            "role": "Fork Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4.5",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.231",
        "ergebnis/composer-normalize": "^2.28",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpspec/phpspec": "^7.2.0",
        "phpstan/phpstan": "^1.8.2",
        "psy/psysh": "^0.11.7",
        "slevomat/coding-standard": "^8.3.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "suggest": {
        "aws/aws-sdk-php": "Allows using the AWS SNS driver"
    },
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\SMS\\": "src/",
            "spec\\Matthewbdaly\\SMS\\": "spec/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
